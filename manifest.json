{
    "manifest_version": 3,
    "name": "studious extension",
    "version": "0.1",
    "description": "Reminder to pay attention in class",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "alarms",
        "notifications"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
        "defaule_title": "pay attention popup!",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "128": "icons/128.png"
        }
    }
}