{
    "manifest_version": 3,
    "name": "studious",
    "version": "1.0",
    "description": "Reminder to pay attention in class",
    "permissions": [
        "notifications",
        "alarms"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "icons/48.png"
        }
    }
}